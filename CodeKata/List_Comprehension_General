fruits = ["apple", "banana", "grape"]
newfruit = []
for x in fruits:
    if "n" in x:
        newfruit.append(x)
print(newfruit)

#or

fruits = ["apple", "banana", "grape", "kiwi"]
newfruit = [x for x in fruits if "i" in x]
print(newfruit)

#or
fruits = ["apple", "banana", "grape", "kiwi"]
newfruit = [x for x in fruits if x != "kiwi"]
print(newfruit)

newnum = [x for x in range(10)]
print(newnum)

newnum = [x for x in range(10) if x <6]
print(newnum)

fruits = ["apple", "banana", "grape", "kiwi"]
newfruit = [x.upper() for x in fruits]
print(newfruit)


fruits = ["apple", "banana", "grape", "kiwi"]
newfruit = [x if x != "banana" else "orange" for x in fruits]
print(newfruit)
